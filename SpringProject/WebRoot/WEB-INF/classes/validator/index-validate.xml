<?xml version="1.0" encoding="UTF-8"?>
<form class="com.focustech.cookinglove.ibator.domain.CoreLogonInfo" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="validate-schema.xsd">
    <fields>
    	<fieldRule name="logUsername">
			<required>
				<errorTipZH>请填写用户名！</errorTipZH>
			</required>
   		</fieldRule>
   		<fieldRule name="logPassword">
			<required>
				<errorTipZH>请填写密码！</errorTipZH>
			</required>
   		</fieldRule>
   		<fieldRule name="nickName">
			<required>
				<errorTipZH>请填写昵称！</errorTipZH>
			</required>
   		</fieldRule>
   		<fieldRule name="emailAddress">
			<required>
				<errorTipZH>请填写邮箱！</errorTipZH>
			</required>
   		</fieldRule>
    </fields>
    <actions>
      <xcase name="reg">
      	<field name="logUsername"/>
      	<field name="logPassword"/>
      	<field name="nickName"/>
      	<field name="emailAddress"/>
      </xcase>
    </actions>
</form>